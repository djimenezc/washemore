{
  "name": "washemo-20",
  "private": false,
  "version": "0.2.1",
  "description": "Logtrust components catalog: Widgets Assortment Solve, Homogeneously (with) Expertise, Modelling Operations",
  "main": "",
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist/*",
    "copy": "node_modules/.bin/copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "copy:lib": "node_modules/.bin/copyfiles -f ./node_modules/react/dist/react.js ./node_modules/react-dom/dist/react-dom.js ./node_modules/babel-standalone/babel.js ./src/library.html ./dist/lib/",
    "dist": "npm run clean & npm run dist:app & npm run dist:inline & npm run dist:lib",
    "dist:app": "npm run copy & node_modules/.bin/webpack --env=dist",
    "dist:inline": "gulp",
    "dist:lib": "npm run copy:lib &  node_modules/.bin/webpack --env=lib",
    "lint": "node_modules/.bin/eslint ./src",
    "posttest": "npm run lint",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && npm publish && git push --follow-tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && npm publish && git push --follow-tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "serve:lib": "node_modules/.bin/http-server -p 8001 ./dist/lib",
    "start": "npm run serve",
    "test": "node_modules/.bin/karma start",
    "test:watch": "node_modules/.bin/karma start --autoWatch=true --singleRun=false",
    "deps": "npm install --quiet --loglevel warn && ./node_modules/.bin/selenium-standalone install && mkdir ./build/",
    "selenium": "./node_modules/.bin/nightwatch -e chrome"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.logtrust.com/logtrust/washemo"
  },
  "keywords": [
    "widgets",
    "analytics",
    "apps",
    "verticales",
    "display",
    "data",
    "visualization"
  ],
  "homepage": "https://www.logtrust.com",
  "license": "MIT",
  "author": "Logtrust desa@logtrust.com",
